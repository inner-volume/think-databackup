{
  "name": "tp5er/tp5-databackup",
  "description": "Implement the database backup restore",
  "license": "MIT",
  "authors": [
    {
      "name": "tp5er",
      "email": "tp5er@qq.com"
    }
  ],
  "require": {
    "php": ">=7.2.5",
    "ext-zlib": "*",
    "topthink/framework": "6.0.*"
  },
  "autoload": {
    "psr-4": {
      "tp5er\\Backup\\": "src/"
    },
    "files": [
      "src/function.php"
    ]
  },
  "require-dev": {
    "topthink/think-view": "2.*"
  },
  "extra": {
    "think": {
      "services": [
        "tp5er\\Backup\\Service"
      ],
      "config": {
        "backup": "config/backup.php"
      }
    }
  }
}
